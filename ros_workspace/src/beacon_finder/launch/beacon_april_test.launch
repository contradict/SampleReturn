<launch>

    <node name="april_beacon"
        type="beacon_april_node"
        pkg="beacon_finder"
        ns="/processes/beacon"
        >
        <remap from="image_rect" to="/cameras/search/image"/>
        <remap from="/cameras/search/camera_info" to="/cameras/search/info"/>

        <rosparam param="tag_descriptions">
            [
                {
                    id: 407,
                    size: .8128,
                    corners: [-1, -1, 0,
                            1, -1, 0,
                            1, 1, 0,
                            -1, 1, 0] },
                {id: 1, size: 0.163513, frame_id: a_frame},
                {id: 2, size: 0.163513},
                {id: 3, size: 0.163513},
                {id: 4, size: 0.163513},
                {id: 5, size: 0.163513}
            ]
        </rosparam>
    </node>

</launch>
