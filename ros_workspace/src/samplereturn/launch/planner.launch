<?xml version="1.0"?>
<!-- vim: set fenc=utf-8 et sts=4 ft=xml : -->
<launch>
    <node
        name="mover"
        package="navigation"
        type="move_base"
        ns="/planner"
        output="log"
        >
        <remap from="map" to="/navigation/map"/>
        <rosparam file="$(find samplereturn)/config/costmap_common_params.yaml" command="load" ns="local_costmap" />
        <rosparam file="$(find samplereturn)/config/costmap_local_params.yaml" command="load"/>
        <rosparam file="$(find samplereturn)/config/costmap_common_params.yaml" command="load" ns="global_costmap" />
        <rosparam file="$(find samplereturn)/config/costmap_global_params.yaml" command="load"/>
        <rosparam file="$(find samplereturn)/config/base_local_planner_params.yaml" command="load"/>
    </node>

</launch>
