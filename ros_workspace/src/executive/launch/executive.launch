<?xml version="1.0"?>
<!-- vim: set fenc=utf-8 et sts=4 ft=xml : -->
<launch>

    <node
        machine="navigate"
        pkg="executive"
        type="master_executive.py"
        name="master_executive"
        output="log">
        <param name="precached_sample_distance" type="double" value="10.0"/>
        <param name="home_pursuit_complete_distance" type="double" value="4.0"/>
        <param name="maximum_pursuit_error" type="double" value="0.5"/>

        <remap from="img_point" to="/namedpoints"/>
    </node>

</launch>
